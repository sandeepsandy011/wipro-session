import java.util.*;
class ascii
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the character:");
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<=n-1;i++)
		{
			a[i]=sc.nextInt();
		}
		System.out.println("Corresponding values are: ");
		for(int i=0;i<=n-1;i++)
		{
			char b=(char)a[i];
			System.out.print(b+" ");
		}


	}
}
