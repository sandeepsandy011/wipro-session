import java.util.*;
class sort 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number:");
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<=n-1;i++)
		{
			a[i]=sc.nextInt();
		}
		Arrays.sort(a);
		System.out.println("sorted array");
		for(int i=0;i<=n-1;i++)
		{
			System.out.print(a[i]+" ");
		}

		
	}
}
